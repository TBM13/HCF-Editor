<UserControl x:Class="HCF_Editor.UI.ListValueEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HCF_Editor.UI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White" x:Name="Grid">
        <Rectangle Stroke="DarkGray" StrokeThickness="0.3"/>
        <Expander x:Name="Expander">
            <StackPanel x:Name="StackPanel">

            </StackPanel>
        </Expander>
        <Button x:Name="SaveButton" Content="Save" Height="20" VerticalAlignment="Top" Margin="0,2,5,0" IsEnabled="False" HorizontalAlignment="Right" Width="71" Click="SaveButton_Click"/>
    </Grid>
</UserControl>
